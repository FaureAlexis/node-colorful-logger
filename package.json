{
  "name": "node-colorful-logger",
  "version": "1.1.0",
  "description": "A simple logger with colors &amp; timestamp, without dependencies.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "test": "jest",
    "dev": "nodemon",
    "build": "rm -rf build && tsc",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FaureAlexis/node-colorful-logger.git"
  },
  "keywords": [
    "Colorful logger",
    "Logger",
    "Colorful",
    "Color",
    "Node logger",
    "Node colorful logger",
    "Node colorful"
  ],
  "author": "Alexis Faure",
  "license": "ISC",
  "files": [
    "build/**/*"
  ],
  "bugs": {
    "url": "https://github.com/FaureAlexis/node-colorful-logger/issues"
  },
  "homepage": "https://github.com/FaureAlexis/node-colorful-logger#readme",
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3"
  }
}
